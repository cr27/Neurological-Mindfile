#Import Brain Metabolism Data

library(readr)
recon_store_genes_1 <- read_delim("C:/Users/abc/Desktop/recon-store-genes-1.tsv", 
                                  "\t", escape_double = FALSE, trim_ws = TRUE)
View(recon_store_genes_1)

#Convert metabolic data to csv
write.csv(recon_store_genes_1, "/Users/abc/Desktop/neurometabolismdata.csv")
